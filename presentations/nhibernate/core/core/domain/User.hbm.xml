<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="core.domain.User,core" table="Users" lazy="false">
    <id name="Uid" type="Guid">
      <generator class="guid.comb"/>
    </id>
    <property name="FirstName" type="string" length="100" not-null="false"/>
    <property name="Surname" type="string" length="100" not-null="false"/>
    <property name="Email" type="string" length="100" not-null="false"/>    
  </class>
</hibernate-mapping>
